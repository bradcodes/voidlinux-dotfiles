! For Fixing Fonts (May not work)
!Xft.rgba:	rgb
!for normal X11 fonts:
! XFT
!Xft.lcdfilter : lcddefault
!Xft.hintstyle : hintslight
!Xft.hinting   : true
!Xft.antialias : true
!Xft.rgba      : rgb

!*font: xft:Input:style=Light,pixelsize=13
*font: xft:Dejavu Sans Mono:pixelsize=13,style=regular
!*font: xft:Ubuntu Mono:pixelsize=14,style=regular
!*font: xft:Roboto Mono:pixelsize=12,style=regular
! General Settings
urxvt*termName:         rxvt
URxvt*geometry: 75x22
urxvt*depth:            24
urxvt*fading:           0
urxvt*shading:          10
urxvt*tintColor:        #000000
urxvt*cursorColor:      #999999
urxvt*inheritPixmap:    false
urxvt*saveLines:        32767
urxvt*visualBell:       false
urxvt*scrollTtyKeypress:true
urxvt*scrollWithBuffer: false
urxvt*scrollTtyOutput:  false
urxvt*scrollBar:        false
urxvt*loginShell:       true
urxvt*cutchars:         "()*,<>[]{}|'
urxvt*print-pipe:       cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
urxvt*secondaryScroll:  true
urxvt*mapAlert:         true
urxvt*utmpInhibit:      true
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  option-popup,matcher
urxvt*urlLauncher:      /usr/bin/firefox
urxvt*matcher.button:   1
urxvt*internalBorder:	6
urxvt*externalBorder:   1

urxvt*letterspace: -12

*.background: #1f1f1f
*foreground: #c0b18b

!black
*color0:#4A3637
*color8:#4A3637

!red
*color1:#d17b49
*color9:#d17b49

!green
*color2:#7b8748
*color10:#7b8748

!yellow
*color3:#af865a
*color11:#af865a

!blue
*color4:#535C5C
*color12:#535C5C

!magenta
*color5:#775759
*color13:#775759

!cyan
*color6:#6d715e
*color14:#6d715e

!white
*color7:#c0b18b
*color15:#c0b18b
